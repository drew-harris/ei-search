{
  "name": "the-beth-stack",
  "module": "src/index.tsx",
  "type": "module",
  "scripts": {
    "db:push": "bunx drizzle-kit push:pg",
    "db:studio": "bunx drizzle-kit studio",
    "dev": "concurrently \"bun run --watch src/index.tsx\" \"bun run tw:dev\"",
    "scrape": "bun ./src/scrape.ts",
    "tw": "bunx tailwindcss -m -i ./src/styles.css -o ./tailwind-gen/styles.css",
    "tw:dev": "bunx tailwindcss -m -i ./src/styles.css -o ./tailwind-gen/styles.css --watch",
    "algolia": "bun ./src/scripts/uploadalgolia.ts"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "latest",
    "@types/pg": "^8.11.6",
    "bun-types": "latest",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.18",
    "tailwindcss": "^3.4.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/html": "^",
    "@googleapis/youtube": "^10.0.0",
    "@sentry/bun": "^7.108.0",
    "@t3-oss/env-core": "^0.7.1",
    "algoliasearch": "^4.22.1",
    "drizzle-orm": "^0.30.10",
    "elysia": "^",
    "nanoid": "^5.0.7",
    "pg": "^8.11.5",
    "postgres": "^3.4.4",
    "posthog-node": "^3.1.3",
    "zod": "^3.22.4"
  }
}
